    
<% include partials/header %>


	<!--start-header-->


	<!--banner-starts-->
	<div class="banner">
		<div class="container">
			<div class="banner-top">
				<div class="banner-text">
					<h2>Welcome To</h2>
					<h1>Techkie Magazine</h1>
				</div>
			</div>
		</div>
	</div>
	
	<div class="about">
		<div class="container">
				<div class="col-md-8 about-left">
					<div class="about-tre">
					           <% articles.forEach(function(article){ %>

						<div class="a-1">
							<div class="col-md-6 abt-left">
								<a href="single.html"><img src="<%= article.image %>" alt="" /></a>
								<h6>Find The Most</h6>
								<h3><a href="single.html"><%= article.name %></a></h3>
								<p><%= article.descriptions %></p>
								<div class="about-btn">
									<a href="/articles/<%= article.id %>">Read More</a>
								</div>

								<label><%= article.dateN.toDateString() %></label>
							</div>
		
						</div>
	<% }) %> 
					</div>	
				</div>

		</div>
	</div>


<% include partials/footer %>  
